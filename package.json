{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn graphql:types:local && react-app-rewired start --host 0.0.0.0",
    "build": "dotenv -e .env.production react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "eslint": "eslint src --ext .js,.ts,.tsx,.jsx",
    "typecheck": "tsc --noEmit",
    "deploy": "yarn build && firebase deploy",
    "graphql:types:local": "yarn run apollo client:codegen --passthroughCustomScalars --target=typescript --customScalarsPrefix=Api --outputFlat src/types/api --endpoint http://localhost:8008/graphql"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.4.1",
    "@material-ui/styles": "^4.3.3",
    "@sentry/browser": "^5.11.0",
    "@types/color": "^3.0.1",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.138",
    "@types/node": "^11.13.5",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/recompose": "0.30.7",
    "@types/styled-components": "^4.1.18",
    "@types/uniqid": "^4.1.3",
    "@types/yup": "^0.26.23",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "apollo": "^2.22.1",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "bootstrap": "^3.3.5",
    "color": "^3.1.2",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "firebase": "^7.5.0",
    "formik": "^1.5.2",
    "graphql": "^14.3.1",
    "history": "^4.10.1",
    "husky": "^3.0.9",
    "is_js": "^0.9.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "notistack": "^0.9.6",
    "prettier": "^1.18.2",
    "query-string": "^6.9.0",
    "react": "^16.8.6",
    "react-apollo": "^3.1.3",
    "react-app-rewire-aliases": "^0.2.0",
    "react-app-rewire-graphql-tag": "^1.1.0",
    "react-app-rewire-webpack-bundle-analyzer": "^1.1.0",
    "react-app-rewired": "^2.1.3",
    "react-circular-progressbar": "^2.0.2",
    "react-detect-offline": "^2.4.0",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.8.6",
    "react-google-button": "^0.6.0",
    "react-hotjar": "^2.0.4",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "2.1.8",
    "styled-components": "^4.3.2",
    "typescript": "^3.4.4",
    "uniqid": "^5.1.0",
    "yup": "^0.27.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint && yarn typecheck"
    }
  }
}
